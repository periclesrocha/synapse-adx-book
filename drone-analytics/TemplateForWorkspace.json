				"folder": {
					"name": "Experiments"
				},
				"content": {
					"query": "['fleet data']\n| summarize ['Average Temperature'] = avg(CoreTemp) by DeviceState\n| order by ['Average Temperature'] desc ",
					"metadata": {
						"language": "kql"
					},
